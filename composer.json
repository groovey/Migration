{
    "name": "groovey/migration",
    "description": "A KISS migration script using configuration file.",
    "license": "MIT",
    "keywords": ["groovey", "framework", "simple"],
    "authors": [
        {
            "name": "Harold Kim Cantil",
            "email": "pokoot@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "*",
        "symfony/yaml" : "*",
        "symfony/config" : "*",
        "symfony/finder" : "*",
        "pimple/pimple" : "~3.0",
        "illuminate/support": "*",
        "illuminate/database": "~4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "classmap" : [
            "src/models"
        ],
        "psr-4": {
            "Groovey\\Migration\\": "src"
        }
    }

}
